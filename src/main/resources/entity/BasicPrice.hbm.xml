<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.BasicPrice" table="basic_price" schema="homework_2">
        <id name="pid" column="pid"><generator class="increment"/></id>
        <property name="callPrice" column="call_price"/>
        <property name="messagePrice" column="message_price"/>
        <property name="localDataPrice" column="local_data_price"/>
        <property name="domesticDataPrice" column="domestic_data_price"/>
    </class>
</hibernate-mapping>