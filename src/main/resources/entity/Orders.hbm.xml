<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Orders" table="orders" schema="homework_2">
        <id name="orderno" column="orderno"><generator class="increment"/></id>
        <property name="userPhone" column="user_phone"/>
        <property name="pid" column="pid"/>
        <property name="beginTime" column="begin_time"/>
        <property name="endTime" column="end_time"/>
        <property name="renewal" column="renewal"/>
        <property name="applicable" column="applicable"/>
    </class>
</hibernate-mapping>